XKNB:Toggle("æ— é™ä½“åŠ›", "", false, function(state)
    getgenv().AXIN_CONFIG.InfiniteStamina = state
    if state then
        ResourceSystem.StartInfiniteStamina()
        print("ğŸ’ª æ— é™ä½“åŠ›å·²å¯ç”¨")
    else
        ResourceSystem.StopAll()
        print("ğŸ’ª æ— é™ä½“åŠ›å·²ç¦ç”¨")
    end
end)

XKNB:Toggle("æ— é™æ”»å‡»èµ„æº", "", false, function(state)
    getgenv().AXIN_CONFIG.InfiniteComboStamina = state
    if state then
        ResourceSystem.StartInfiniteComboStamina()
        print("ğŸ”¥ æ— é™æ”»å‡»èµ„æºå·²å¯ç”¨")
    else
        print("ğŸ”¥ æ— é™æ”»å‡»èµ„æºå·²ç¦ç”¨")
    end
end)

