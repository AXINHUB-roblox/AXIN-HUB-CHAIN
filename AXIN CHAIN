end)

-- 系统控制部分
local ControlSection = SystemTab:section("系统控制", true)
ControlSection:Label("🎮 系统控制面板")

ControlSection:Button("启动所有功能", "", function()
    EnableAllFeatures()
end)

ControlSection:Button("停止所有功能", "", function()
    DisableAllFeatures()
end)

ControlSection:Button("重置设置", "", function()
    ResetAllSettings()
end)

ControlSection:Button("保存配置", "", function()
    SaveConfiguration()
end)

ControlSection:Button("加载配置", "", function()
    LoadConfiguration()
end)

-- 关于信息部分
local AboutSection = SystemTab:section("关于", true)
AboutSection:Label("AXIN HUB X959")
AboutSection:Label("版本: v4.0-ChainEnraged")
AboutSection:Label("游戏: CHAIN [ENRAGED]")
AboutSection:Label("作者: 冷秋")
AboutSection:Label("构建: Premium")

AboutSection:Button("检查更新", "", function()
    CheckForUpdates()
end)

